{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col-sm-3">
            <img src="{{ asset('img/article/' ~ article.image) }}" class="img-fluid" alt="">
        </div>
        <div class="col-sm-9">
            <h3>{{ article.name }}</h3>
            <p>{{ article.description }}</p>
            <p> Continent : {{ continent }}</p>
        </div>
    </div>
{% endblock %}







